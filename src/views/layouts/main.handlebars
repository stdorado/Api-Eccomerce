<html lang="en">

  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Inmobiliaria Don-Mario</title>
    <link href="/dist/output.css" rel="stylesheet">
  </head>

  <body>
<nav class="bg-transparent">
    <div class="mx-auto max-w-7xl px-4 sm:px-6 lg:px-8">
      <div class="flex items-center justify-between h-16">
        <!-- Logo -->
        <a href="/" class="flex-shrink-0 flex items-center">
          <svg xmlns="http://www.w3.org/2000/svg" width="40" height="40" fill="black" class="h-8 w-auto transform hover:scale-110 transition-transform" viewBox="0 0 16 16">
            <path d="M8.707 1.5a1 1 0 0 0-1.414 0L.646 8.146a.5.5 0 0 0 .708.708L8 2.207l6.646 6.647a.5.5 0 0 0 .708-.708L13 5.793V2.5a.5.5 0 0 0-.5-.5h-1a.5.5 0 0 0-.5.5v1.293L8.707 1.5Z"/>
            <path d="m8 3.293 4.712 4.712A4.5 4.5 0 0 0 8.758 15H3.5A1.5 1.5 0 0 1 2 13.5V9.293l6-6Z"/>
            <path d="M12.5 16a3.5 3.5 0 1 0 0-7 3.5 3.5 0 0 0 0 7Zm1.679-4.493-1.335 2.226a.75.75 0 0 1-1.174.144l-.774-.773a.5.5 0 0 1 .708-.707l.547.547 1.17-1.951a.5.5 0 1 1 .858.514Z"/>
          </svg>
          <span class="ml-2 text-black text-xl font-semibold">Don Mario</span>
        </a>

        <!-- Menú móvil -->
        <div class="flex items-center sm:hidden">
          <button id="mobile-menu-button" class="text-gray-400 hover:text-white focus:outline-none focus:ring-2 focus:ring-inset focus:ring-white">
            <span class="sr-only">Open main menu</span>
            <svg class="h-6 w-6 transform hover:rotate-90 transition-transform" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 12h16M4 18h16"></path>
            </svg>
          </button>
        </div>

        <!-- Menú de navegación -->
        <div class="hidden sm:flex sm:space-x-4">
          <a href="/" class="text-black hover:bg-gray-700 hover:text-white px-3 py-2 text-sm font-medium transition-colors duration-300">Home</a>
          <a href="/products" class="text-black hover:bg-gray-700 hover:text-white px-3 py-2 text-sm font-medium transition-colors duration-300">Propiedades</a>
          <a href="/cart" class="text-black hover:bg-gray-700 hover:text-white px-3 py-2 text-sm font-medium transition-colors duration-300">Cart</a>
          <a href="/realtimeProducts" class="text-black hover:bg-gray-700 hover:text-white px-3 py-2 text-sm font-medium transition-colors duration-300">Update Products</a>
        </div>

        <!-- Iconos -->
        <div class="hidden sm:flex sm:items-center sm:ml-6">
          <button class="text-gray-400 hover:text-black focus:outline-none focus:ring-2 focus:ring-white focus:ring-offset-2 focus:ring-offset-gray-800 transition-colors duration-300">
            <span class="sr-only">View notifications</span>
            <svg class="h-6 w-6 transform hover:scale-110 transition-transform" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M14.857 17.082a23.848 23.848 0 005.454-1.31A8.967 8.967 0 0118 9.75v-.7V9A6 6 0 006 9v.75a8.967 8.967 0 01-2.312 6.022c1.733.64 3.56 1.085 5.455 1.31m5.714 0a24.255 24.255 0 01-5.714 0m5.714 0a3 3 0 11-5.714 0"></path>
            </svg>
          </button>

          <!-- Perfil del usuario -->
          <div class="ml-3 relative">
            <button class="flex items-center text-sm font-medium text-white focus:outline-none focus:ring-2 focus:ring-white focus:ring-offset-2 focus:ring-offset-gray-800 transition-colors duration-300">
              <span class="sr-only">Open user menu</span>
              <img class="h-8 w-8 rounded-full transform hover:scale-110 transition-transform" src="https://luisgyg.com/wp-content/uploads/2022/12/WhatsApp-Image-2022-12-06-at-06.25.46-1.jpeg" alt="User Image">
            </button>
          </div>
        </div>
      </div>
    </div>

    <!-- Menú móvil (oculto por defecto) -->
    <div class="sm:hidden" id="mobile-menu">
      <div class="px-2 pt-2 pb-3 space-y-1">
        <a href="/" class="text-black hover:bg-gray-700 hover:text-white block px-3 py-2 text-base font-medium transition-colors duration-300">Home</a>
        <a href="/products" class="text-black hover:bg-gray-700 hover:text-white block px-3 py-2 text-base font-medium transition-colors duration-300">Propiedades</a>
        <a href="/api/carts" class="text-black hover:bg-gray-700 hover:text-white block px-3 py-2 text-sm font-medium transition-colors duration-300">Cart</a>
        <a href="/realtimeProducts" class="text-black hover:bg-gray-700 hover:text-white block px-3 py-2 text-sm font-medium transition-colors duration-300">Update Products</a>
      </div>
    </div>
  </div>
</nav>
  
    {{{body}}}

 
 
<script src="../../socket.io/socket.io.js"></script>

    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
      crossorigin="anonymous"
    ></script>
    <script src="https://cdn.tailwindcss.com"></script>
  </body>

</html>